<h1>New Snippet</h1>

<%= form_for @snippet do |f| %>
<div>
  <%= f.label :title %>
  <%= f.text_field :title %>
</div>
<div>
  <%= f.label :kind_id %>
  <%= f.collection_select(:kind_id, Kind.all, :id, :name) %>
</div>
<div>
  <%= f.label :work %>
  <%= f.text_area :work %>
</div>
<%= f.submit %>
<% end %>
